<div class="container">
    <div class="row">
        {{#each images}}
        <div class="col s12 m6 l4">
            <div class="card medium">
                <div class="card-image ">
                    <img class="responsive-image" src="/api/photos/{{key}}">
                </div>                    
                <div class="card-action">
                    <h3>{{username}} <span class="right material-icons">favorite</span></h3>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>